<h2>7 Day Forecast</h2>
  {{#each list}}
  <div class="sevenDayDiv col-sm-6 col-md-4">
    <div class="thumbnail">
      <div><a href="#"><span class="glyphicon glyphicon-floppy-disk" data-toggle="tooltip" data-placement="left" title="Save Forecast"></span></a>{{dt}}</div>
      <div>{{main.temp}}</div>
      {{#each weather}}
      <h1>{{main}}</h1>
      <div>{{description}}</div>
      {{/each}}
      <div>
        <span>{{main.pressure}}</span>
        <span>{{wind.speed}}</span>
      </div>
    </div>
  </div>
  {{/each}}